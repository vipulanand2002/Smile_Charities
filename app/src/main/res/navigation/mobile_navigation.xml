<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.smilecharities.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.smilecharities.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/nav_category"
        android:name="com.example.smilecharities.ui.category.CategoryFragment"
        android:label="@string/category"
        tools:layout="@layout/fragment_category" />
    <fragment
        android:id="@+id/nav_myCartFragment"
        android:name="com.example.smilecharities.MyCartFragment"
        android:label="@string/mycart"
        tools:layout="@layout/fragment_my_cart" />
    <fragment
        android:id="@+id/nav_newProductsFragment"
        android:name="com.example.smilecharities.ui.predictions.MyPredictionsFragment"
        android:label="@string/my_predictions"
        tools:layout="@layout/fragment_my_predictions" />
    <fragment
        android:id="@+id/nav_myOrdersFragment"
